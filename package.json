{
  "name": "nano-snippets",
  "displayName": "Nano Snippets",
  "description": "Simple snippets used by Nano for faster development",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.41.0"
  },
  "publisher": "DevNano",
  "categories": [
    "Snippets"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/npm/cli.git"
  },
  "contributes": {
    "snippets": [
      {
        "language": "html",
        "path": "./snippets/snippets.json"
